  <validgames>
    <validgame>Starcraft 2</validgame>
  </validgames>
  <doIts>
    <doIt keyword="ai_send_wave">
    AIAttackWaveSend(&lt;playerNum,1&gt;, &lt;delayTime,2&gt;,
    &lt;wait,3&gt;);</doIt>
    <doIt keyword="ai_setTargetRegion_wave">
    AIAttackWaveSetTargetRegion(&lt;playerNum,1&gt;,
    &lt;region,2&gt;, &lt;wait,3&gt;);</doIt>
    <doIt keyword="ai_set_wave">AIWaveSet(&lt;playerNum,1&gt;,
    &lt;index,2&gt;, &lt;wave,3&gt;);</doIt>
    <doIt keyword="ai_setType_wave">AIWaveSetType(&lt;wave,1&gt;,
    &lt;typeID,2&gt;, &lt;waveTarget,3&gt;);</doIt>
    <doIt keyword="ai_addUnit_wave">AIWaveAddUnit(&lt;wave,1&gt;,
    &lt;unit,1&gt;);</doIt>
    <doIt keyword="ai_wave_addUnitGroup">//Adds every unit in
    unitGroup to the wave UnitGroupLoopBegin(&lt;unitGroup,1&gt;);
    while (!UnitGroupLoopDone()) { AIWaveAddUnit(&lt;wave,2&gt;,
    UnitGroupLoopCurrent()); UnitGroupLoopStep(); }
    UnitGroupLoopEnd();</doIt>
    <doIt keyword="unitGroup_loopBegin">
    UnitGroupLoopBegin(&lt;unitGroup,1&gt;);</doIt>
    <doIt keyword="unitGroup_loopDone">UnitGroupLoopDone();</doIt>
    <doIt keyword="unitGroup_loopStep">UnitGroupLoopStep();</doIt>
    <doIt keyword="equals">&lt;arg1,1&gt; == &lt;arg2,2&gt;</doIt>
  </doIts>
