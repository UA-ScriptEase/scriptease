                <Type>Emotion</Type>
              </Types>
              <Binding flavour="constant">
                <Type>Emotion</Type>
                <Value>1</Value>
              </Binding>
            </KnowIt>
          </Parameters>
          <Code>
            <Line>
              <Literal>var seAnimationController : SEAnimationController = SEAnimationController.GetAnimationController( </Literal>
              <Scope data="ARGUMENT" ref="Character">
                <Fragment data="NAME"/>
              </Scope>
              <Literal>);</Literal>
            </Line>
            <Line>
              <Literal>switch (</Literal>
              <Scope data="ARGUMENT" ref="Emotion">
                <Fragment data="NAME"/>
              </Scope>
              <Literal>) {</Literal>
            </Line>
            <Indent>
              <Line>
                <Literal>case 1:</Literal>
              </Line>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>seAnimationController.happyWeight = 1;</Literal>
                </Line>
              </Indent>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>seAnimationController.sadWeight = 0;</Literal>
                </Line>
              </Indent>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>seAnimationController.angryWeight = 0;</Literal>
                </Line>
              </Indent>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>seAnimationController.afraidWeight = 0;</Literal>
                </Line>
              </Indent>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>break;</Literal>
                </Line>
              </Indent>
            </Indent>
            <Indent>
              <Line>
                <Literal>case 2:</Literal>
              </Line>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>seAnimationController.happyWeight = 0;</Literal>
                </Line>
              </Indent>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>seAnimationController.sadWeight = 0.5;</Literal>
                </Line>
              </Indent>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>seAnimationController.angryWeight = 0;</Literal>
                </Line>
              </Indent>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>seAnimationController.afraidWeight = 0;</Literal>
                </Line>
              </Indent>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>break;</Literal>
                </Line>
              </Indent>
            </Indent>
            <Indent>
              <Line>
                <Literal>case 3:</Literal>
              </Line>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>seAnimationController.happyWeight = 0;</Literal>
                </Line>
              </Indent>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>seAnimationController.sadWeight = 0;</Literal>
                </Line>
              </Indent>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>seAnimationController.angryWeight = 0.5;</Literal>
                </Line>
              </Indent>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>seAnimationController.afraidWeight = 0;</Literal>
                </Line>
              </Indent>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>break;</Literal>
                </Line>
              </Indent>
            </Indent>
            <Indent>
              <Line>
                <Literal>case 4:</Literal>
              </Line>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>seAnimationController.happyWeight = 0;</Literal>
                </Line>
              </Indent>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>seAnimationController.sadWeight = 0;</Literal>
                </Line>
              </Indent>
            </Indent>
            <Indent>
              <Indent>
                <Line>
                  <Literal>seAnimationController.angryWeight = 0;</Literal>
