    <ScriptIt>
      <Name>Set &lt;Character&gt;&apos;s emotion to &lt;Emotion&gt;</Name>
      <Visible>true</Visible>
      <Enabled>true</Enabled>
      <Labels>
        <Label>ANI</Label>
      </Labels>
      <Children/>
      <CodeBlocks>
        <CodeBlockSource>
          <Name>Set &lt;Character&gt;&apos;s emotion to &lt;Emotion&gt;</Name>
          <Visible>true</Visible>
          <Enabled>true</Enabled>
          <Labels/>
          <Id>11</Id>
          <Types/>
          <Parameters>
            <KnowIt>
              <Name>Character</Name>
              <Visible>true</Visible>
              <Enabled>true</Enabled>
              <Labels/>
              <Types>
                <Type>GameObject</Type>
              </Types>
            </KnowIt>
